<div class="flex justify-center mt-2">
<% if(success && success.length){%>
<div class=" text-[24px] bg-green-200 border border-green-500 rounded-lg p-2 w-[30%]  flex justify-center">
    <%=success%>
</div><%}%>

<% if(error && error.length){%>
<div class=" text-[24px] bg-red-200 border border-red-500 rounded-lg p-2 w-[30%] flex justify-center">
    <%=error%>
</div><%}%>
</div>


